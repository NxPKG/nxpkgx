{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "nxpkg run build",
    "clean": "nxpkg run clean",
    "dev": "nxpkg run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "nxpkg run lint",
    "test": "nxpkg run test"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.8.8",
    "nxpkg": "^1.9.3"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
